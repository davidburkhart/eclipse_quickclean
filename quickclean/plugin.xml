<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <command
            name="Clean All Command"
            id="cleanbutton.commands.cleanAllCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="cleanbutton.commands.cleanAllCommand"
            class="cleanbutton.handlers.CleanAllHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="cleanbutton.commands.cleanAllCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M3+M4+c"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="cleanbutton.toolbars.cleanAllToolbar">
            <command
                  commandId="cleanbutton.commands.cleanAllCommand"
                  icon="icons/edit-clear.png"
                  tooltip="Clean and rebuild workspace"
                  id="cleanbutton.commands.cleanAllCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>

</plugin>
