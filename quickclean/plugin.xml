<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <command
            name="Clean All Command"
            id="quickclean.commands.refreshAndCleanAllCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="quickclean.commands.refreshAndCleanAllCommand"
            class="quickclean.handlers.RefreshAndCleanAllHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="quickclean.commands.refreshAndCleanAllCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M3+M4+c"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="quickclean.toolbars.maintoolbar">
            <command
                  commandId="quickclean.commands.refreshAndCleanAllCommand"
                  icon="icons/edit-clear.png"
                  tooltip="Refresh, clean and rebuild workspace"
                  id="quickclean.commands.refreshAndCleanAllCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabs">
      <tab
            class="quickclean.launchconfiguration.LaunchConfigurationTab"
            group="org.eclipse.ant.ui.launchConfigurationTabGroup.ant"
            id="quickclean.launchconfigurationtab"
            name="quickclean.launchconfigurationtab">
      </tab>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="quickclean.EarlyStartup">
      </startup>
   </extension>

</plugin>
